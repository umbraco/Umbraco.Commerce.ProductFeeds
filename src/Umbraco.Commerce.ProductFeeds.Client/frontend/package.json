{
    "name": "umbraco-commerce-product-feeds",
    "version": "17.0.0-rc1",
    "description": "A product feeds generator for Umbraco Commerce",
    "main": "./src/index.ts",
    "type": "module",
    "scripts": {
        "copy-statics": "copyfiles -u 1 \"./src/umbraco-package.json\" \"../../Umbraco.Commerce.ProductFeeds.Web/wwwroot/\"",
        "build": "vite build && npm run copy-statics",
        "watch": "npm run copy-statics && vite build --watch --mode development",
        "lint": "eslint . --fix",
        "gen-api": "openapi-ts"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@hey-api/openapi-ts": "^0.85.2",
        "@types/eslint__js": "^9.14.0",
        "autoprefixer": "^10.4.21",
        "copyfiles": "^2.4.1",
        "eslint": "^9.39.0",
        "globals": "^16.5.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.16",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.2",
        "vite": "^6.4.1",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@hey-api/client-axios": "^0.9.1",
        "@umbraco-cms/backoffice": "^17.0.0-rc2",
        "@umbraco-commerce/backoffice": "17.0.0-rc2",
        "nanoid": "^5.1.6"
    }
}
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Umbraco Commerce Product Feeds</Title>
    <TargetFramework>net9.0</TargetFramework>
    <Description>Expand your product reach and show your products to a larger audience with the help of product feeds.</Description>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageReadmeFile>README-nuget.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/umbraco/Umbraco.Commerce.ProductFeeds</RepositoryUrl>
    <PackageTags>umbraco commerce product-feeds umbraco-marketplace</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Class1.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Umbraco.Commerce.ProductFeeds.Startup\Umbraco.Commerce.ProductFeeds.Startup.csproj" />
    <ProjectReference Include="..\Umbraco.Commerce.ProductFeeds.Web\Umbraco.Commerce.ProductFeeds.Web.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README-nuget.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
